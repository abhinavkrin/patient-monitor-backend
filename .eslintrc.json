{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "error",
        "eqeqeq": "warn",
        "no-unused-vars": "warn",
		"eol-last": "warn"
    },
    "globals": {
        "__dirname": "readonly",
        "exports":"readonly",
        "require": "readonly",
        "module": "readonly",
        "console": "readonly",
        "process": "readonly"
    }
}
